programa
{

	funcao inicio()
	{
		inteiro n = 1
		escreva("Operadores de Atribuição\n")
		escreva("---------------------------------\n")
		escreva(n,"\n")
		n+=1
		escreva(n,"\n")
		n-=1
		escreva(n,"\n")
		n*=100
		escreva(n,"\n")
		n/=2
		escreva(n,"\n")
		n ++
		escreva(n,"\n")
		n --
		escreva(n)
	}
}
 